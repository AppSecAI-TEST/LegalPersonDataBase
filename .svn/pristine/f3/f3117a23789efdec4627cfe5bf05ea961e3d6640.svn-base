	<select id="getList" parameterType="String" resultMap="BaseResultMap">
      SELECT
      xmmc,xmlb,jsnr,xmztz
      FROM frk.cg_qt_cjxm_valid
      WHERE frwybs = #{frwybs}
    </select>
    
    <select id="selectCjxmDetail" parameterType="Map" resultMap="BaseResultMap">
		Select frwybs,xmmc,xmdw,xmlb,jsnr,xmztz from frk.cg_qt_cjxm_valid
		<where>
			<if test="xmmc!=null and xmmc!=''">
				AND xmmc LIKE '%'||#{xmmc}||'%'
			</if>
			<if test="xmdw!=null and xmdw!=''">
				AND xmdw LIKE '%'||#{xmdw}||'%'
			</if>
			AND xmmc IS NOT NULL
		</where>	
	</select>
