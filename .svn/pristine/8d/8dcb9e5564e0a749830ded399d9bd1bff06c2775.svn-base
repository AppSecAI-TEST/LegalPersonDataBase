package com.icinfo.frk.api.dto;

import com.fasterxml.jackson.annotation.JsonFormat;
import com.fasterxml.jackson.annotation.JsonIgnore;

import java.util.Date;

/**
 * 法人校核Dto
 * Created by yushunwei on 2017-07-07.
 */
public class FrCheckDto {
    //法人唯一标识
    @JsonIgnore
    private String frwybs;

    // 企业单位名称
    private String qydwmc;

    // 统一社会信用代码
    private String tyxydm;

    // 组织机构代码
    private String zzjgdm;

    // 核准机构
    private String hzjg;

    // 核准时间
    @JsonFormat(pattern = "yyyy-MM-dd", timezone = "GMT+8")
    private Date hzsj;

    // 法定代表人
    private String fddbr;

    // 法人类型
    private String frlx;

    // 注册地址
    private String zcdz;

    // 状态
    private String zt;

    public String getQydwmc() {
        return qydwmc;
    }

    public void setQydwmc(String qydwmc) {
        this.qydwmc = qydwmc;
    }

    public String getTyxydm() {
        return tyxydm;
    }

    public void setTyxydm(String tyxydm) {
        this.tyxydm = tyxydm;
    }

    public String getZzjgdm() {
        return zzjgdm;
    }

    public void setZzjgdm(String zzjgdm) {
        this.zzjgdm = zzjgdm;
    }

    public String getHzjg() {
        return hzjg;
    }

    public void setHzjg(String hzjg) {
        this.hzjg = hzjg;
    }

    public Date getHzsj() {
        return hzsj;
    }

    public void setHzsj(Date hzsj) {
        this.hzsj = hzsj;
    }

    public String getFddbr() {
        return fddbr;
    }

    public void setFddbr(String fddbr) {
        this.fddbr = fddbr;
    }

    public String getFrlx() {
        return frlx;
    }

    public void setFrlx(String frlx) {
        this.frlx = frlx;
    }

    public String getZcdz() {
        return zcdz;
    }

    public void setZcdz(String zcdz) {
        this.zcdz = zcdz;
    }

    public String getZt() {
        return zt;
    }

    public void setZt(String zt) {
        this.zt = zt;
    }

    public String getFrwybs() {
        return frwybs;
    }

    public void setFrwybs(String frwybs) {
        this.frwybs = frwybs;
    }
}
