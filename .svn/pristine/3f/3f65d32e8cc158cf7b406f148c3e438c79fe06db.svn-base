}

.side-bar li.cur {
    background-color: #ff6600;
}

.main {
    float: left;
    width: 1040px;
    background-color: #344170;
    padding: 10px;
    box-sizing: border-box;
}

.main .panel {
    border: 1px solid #274eda;
}

.chart-hd {
