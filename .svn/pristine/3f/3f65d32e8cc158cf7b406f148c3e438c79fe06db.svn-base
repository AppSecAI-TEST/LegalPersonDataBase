body {
    background: #112057;
    color: #333;
    margin: 1em auto 4em auto;
    position: relative;
    width: 100%;
    font-size: 12px;
    overflow-x: hidden;
    font-family: Microsoft YaHei, Helvetica, Arial, sans-serif;
}

html, body, div, span, applet, select, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, caption, input {
    margin: 0;
    padding: 0;
    border: 0;
    outline: 0;
}

.wrap {
    width: 1230px;
    margin: 25px auto 0;
    overflow: hidden;
    position: relative;
}

.border-tl, .border-tr, .border-bl, .border-br {
    position: absolute;
    border: 1px solid #47a3ff;
    width: 11px;
    height: 11px;
}

.border-tl {
    left: 0;
    top: 0;
    border-right: 0;
    border-bottom: 0;
}

.border-tr {
    right: 0;
    top: 0;
    border-left: 0;
    border-bottom: 0;
}

.border-bl {
    left: 0;
    bottom: 0;
    border-right: 0;
    border-top: 0;
}

.border-br {
    right: 0;
    bottom: 0;
    border-left: 0;
    border-top: 0;
}

#chart-table, .table {
    float: left;
    margin-right: 10px;
    box-sizing: border-box;
}

#chart-table {
    height: 0px;
    width: 240px;
    border-right: 1px solid #7a7f8f;
    border-left: 1px solid #7a7f8f;
    border-bottom: 1px solid #7a7f8f;
    margin-left: 10px;
    position: relative;
}

#chart-table hr {
    width: 1px;
    overflow: hidden;
    border: 0;
    border-left: 1px solid #7a7f8f;
    height: 0px;
    position: absolute;
    left: 38px;
    top: -6px;
}

.table-list {
    height: 68px;
    border-left: 1px solid #bccbda;
    margin: 10px 0;
    box-sizing: border-box;
}

.table-list li {
    float: left;
    margin: 0;
    list-style: none;
    width: 234px;
    cursor: pointer;
    color: #0e4791;
    min-height: 18px;
}

.table-list li em {
    font-style: normal;
}

.table-list li i {
    color: #333;
}

.table-select-box {
    width: 568px;
    margin: 20px auto 0;
    font-size: 14px;
}

.table-select-box select {
    width: 408px;
    height: 30px;
    border: 1px solid #abbdd0;
    color: #999;
    font-size: 14px;
    background-color: #fff;
}

.table-select-box input {
    background-color: #3a8eec;
    border-radius: 5px;
    color: #fff;
    width: 66px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    cursor: pointer;
}

.table {
    width: 260px;
    height: 88px;
    padding-left: 10px;
}

.table .title {
    font-size: 20px;
    color: #164f91;
    text-align: center;
    line-height: 54px;
}

#chart-sankey, .table-list {
    width: 700px;
    float: left;
}

#chart-sankey {
    height: 646px;
    background-color: #fff;
}

.node rect {
    fill-opacity: .9;
    /*shape-rendering: crispEdges;*/
    shape-rendering: optimizeSpeed;
}

.node text {
    pointer-events: none;
    /*text-shadow: 0 1px 0 #fff;*/
}

.link {
    fill: none;
    stroke: #000;
    stroke-opacity: .2;
}

.link:hover, .link.cur {
    stroke: red !important;
    stroke-opacity: .6 !important;
}

#chart-sankey svg {
    font: 12px sans-serif;
}

line {
    fill: none;
    shape-rendering: crispEdges;
}

.node2 .line {
    stroke: #bebebe;
}

.node2 .circle {
    fill: #bebebe;
    stroke: #7c7c7c;
}

.node2.cur text {
    fill: red;
}

.node2.cur .line {
    stroke: red;
    stroke-opacity: .6;
}

.node2.cur .circle {
    fill: red;
    stroke: red;
    stroke-opacity: .6;
}

i {
    font-style: normal;
}

.table {
    line-height: 26px;
}

.table .row1 {
    width: 40px;
    float: left;
    border: 1px solid #7a7f8f;
    box-sizing: border-box;
    text-align: center;
    height: 34px;
    line-height: 34px;
}

.table .row2 {
    width: 200px;
    float: left;
    height: 32px;
    line-height: 32px;
}

.table .row2 .row-item {
    width: 20%;
    float: left;
    text-align: center;
}

.table .row2 .row-item .item {
    border: 1px solid #7a7f8f;
    border-left: 0;
}

.arc text {
    font: 12px sans-serif;
    text-anchor: middle;
}

.arc path {
    stroke: #fff;
}

.chart-pie {
    width: 400px;
    height: 300px;
    overflow: hidden;
    float: left;
    margin-top: 30px;
}

.hide {
    display: none;;
}

.side-bar {
    float: left;
    width: 190px;
    background-color: #293768;
    border-right: 1px solid #24305c;
    box-sizing: border-box;
    height: 766px;
}

.side-bar .title {
    color: #fff;
    font-size: 18px;
    text-align: center;
    line-height: 72px;
}

.side-bar li {
    cursor: pointer;
